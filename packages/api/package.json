{
    "name": "sbir-gen-api",
    "version": "0.0.1",
    "description": "SBIRGEN Server",
    "main": "dist/index",
    "types": "dist/index.d.ts",
    "bin": {
        "sibr-gen": "./bin/run"
    },
    "files": [
        "bin",
        "dist",
        "npm-shrinkwrap.json",
        "oclif.manifest.json",
        "oauth2.html"
    ],
    "oclif": {
        "bin": "sbir-gen",
        "commands": "./dist/commands"
    },
    "scripts": {
        "build": "tsc",
        "start": "cd bin && ./run start",
        "dev": "concurrently \"yarn watch\" \"nodemon",
        "oclif-dev": "cd bin && ./dev start",
        "postpack": "shx rm -f oclif.manifest.json",
        "prepack": "yarn build && oclif manifest && oclif readme",
        "typeorm": "typeorm-ts-node-commonjs",
        "watch": "tsc --watch",
        "version": "oclif readme && git add README.md"
    },
    "keywords": [],
    "homepage": "https://genwith.ai",
    "author": {
        "name": "SBIRGEN",
        "email": "info@genwith.ai"
    },
    "engines": {
        "node": ">=18.20.3"
    },
    "license": "SEE LICENSE IN LICENSE.md",
    "dependencies": {
        "@oclif/core": "^1.13.10",
        "@types/jsonwebtoken": "^9.0.2",
        "@types/md5": "^2.3.2",
        "@types/node": "^20.14.10",
        "async-mutex": "^0.4.0",
        "axios": "1.6.2",
        "class-transformer": "^0.5.1",
        "cors": "^2.8.5",
        "cron": "^3.1.7",
        "crypto-js": "^4.1.1",
        "dotenv": "^16.0.0",
        "ejs": "2.7.4",
        "express": "^4.17.3",
        "express-basic-auth": "^1.2.1",
        "express-rate-limit": "^6.9.0",
        "jsonwebtoken": "^9.0.1",
        "md5": "^2.3.0",
        "moment-timezone": "^0.5.34",
        "multer": "^1.4.5-lts.1",
        "mysql": "^2.18.1",
        "pg": "^8.11.1",
        "pptxgenjs": "^3.12.0",
        "reflect-metadata": "^0.1.13",
        "sanitize-html": "^2.11.0",
        "socket.io": "^4.6.1",
        "sqlite3": "^5.1.6",
        "typeorm": "^0.3.6",
        "uuid": "^9.0.1",
        "winston": "^3.9.0"
    },
    "devDependencies": {
        "@types/cors": "^2.8.12",
        "@types/crypto-js": "^4.1.1",
        "@types/ejs": "^2",
        "@types/multer": "^1.4.7",
        "@types/sanitize-html": "^2.9.5",
        "concurrently": "^7.1.0",
        "nodemon": "^2.0.15",
        "oclif": "^1.13.10",
        "shx": "^0.3.3",
        "ts-node": "^10.7.0",
        "typescript": "^4.8.4"
    }
}
